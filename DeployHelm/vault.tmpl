replicaCount: ${replicas}
vault:
  dev: False
  config: 
    storage:
      etcd:
        address: "http://${etcd_clusterip}:${etcd_clusterport}"
        etcd_api: "v3"
        ha_enabled: "true"
        username: "root"
        password: "${etc_rootpwd}"
        path: "/vault/"
serviceAccount:    
  create: True
extraEnvironmentVars:
  VAULT_ADDR: ${VAULT_ADDR}
  BLACKCATS: BLACKCATS
ui:
  enabled: true
  serviceType: LoadBalancer